@model GetVehicleDTO

@{
    ViewData["Title"] = "Details";
    Layout = null;
}


<div class="card" style="width: 31rem;">
    <img class="card-img-top w-100" src="~/images/Mercedes.jpg" alt="Card image cap">
    <div class="card-body">
        <p>Owner: @Model.Owner</p>
        <p>Colour: @Model.Colour</p>
        <p>VIN: @Model.VIN</p>
        <p>Number plate: @Model.NumberPlate</p>
    </div>
</div>