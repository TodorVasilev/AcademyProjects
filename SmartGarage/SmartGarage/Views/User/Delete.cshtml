@model GetUserDTO

@{
    ViewData["Title"] = "Delete";
    Layout = null;
}

<div class="col-12">
    <h3>Are you sure you want to delete this User?</h3>
    <div class="m-0 pb-3">
        <form class="text-mid" asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" />
        </form>
    </div>
</div>
